{
  "name": "node-db4-project",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a tt16-secret-recipes",
    "rollbackh": "heroku run knex migrate:rollback -a tt16-secret-recipes",
    "databaseh": "heroku pg:psql -a tt16-secret-recipes",
    "seedh": "heroku run knex seed:run -a tt16-secret-recipes",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LambdaSchool/node-db4-project.git"
  },
  "dependencies": {
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.7",
    "knex-cleaner": "^1.3.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
